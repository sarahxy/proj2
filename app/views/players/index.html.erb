<div class="container">
  <div class="row">
    <h3>Players</h3>
    <div class="column">
      <table class="table">
        <tbody>
          <% @players.each do |player| %>
            <tr>
              <%= render player %>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
